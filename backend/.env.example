# Database URL for PostgreSQL
# For local development:
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/monopoly

# For Docker:
# DATABASE_URL=postgresql+asyncpg://postgres:postgres@db:5432/monopoly

# For Railway (will be set automatically by Railway when you add PostgreSQL):
# DATABASE_URL=<provided_by_railway>

# Telegram Bot Token (optional for anonymous auth)
BOT_TOKEN=

# Debug mode
DEBUG=true

# CORS Origins
ALLOW_ORIGINS=*
